{
    "manifest_version": 2,
    "name": "Auto Close Tab",
    "version": "1.0",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "permissions": ["tabs"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  